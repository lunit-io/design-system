{
  "name": "@lunit/design-system",
  "version": "1.0.0-a.2",
  "description": "Lunit Design System",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/types/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/cjs/components/*/index.js",
      "default": "./dist/cjs/components/*/index.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/components/*/index.d.ts",
        "dist/types/index.d.ts"
      ]
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lunit-io/design-system.git",
    "directory": "packages/design-system"
  },
  "scripts": {
    "build:prepare": "tsc -p tsconfig.build.json",
    "build:cjs": "webpack -c webpack/cjs.config.js",
    "build": "yarn build:prepare && yarn build:cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic"
  },
  "nx": {
    "targets": {
      "storybook": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.11.3",
    "@mui/utils": "^5.11.3",
    "@mui/x-date-pickers": "^5.0.16",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-storysource": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/theming": "^6.4.22",
    "@types/lodash": "^4.14.182",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.7.0",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-addon-pseudo-states": "^1.15.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@lunit/design-system-icons": "*"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0",
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
