{
  "name": "@lunit/design-system-icons",
  "version": "1.2.1",
  "description": "Icons for Lunit design system",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/*/index.js",
      "require": "./dist/cjs/*/index.js",
      "default": "./dist/cjs/*/index.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/*/index.d.ts",
        "dist/types/index.d.ts"
      ]
    }
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/lunit-io/design-system.git",
    "directory": "packages/design-system-icons"
  },
  "homepage": "https://design-system-icons.d.lunit.io/",
  "scripts": {
    "test": "jest",
    "dev": "node ./builder.mjs && webpack --watch & yarn storybook",
    "build:prepare": "node ./builder.mjs && tsc -p tsconfig.build.json",
    "build:esm": "webpack -c webpack/esm.config.js",
    "build:cjs": "webpack -c webpack/cjs.config.js",
    "build": "yarn build:prepare && yarn build:esm && yarn build:cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --docs && npx -y storybook extract "
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.11.3",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "babel-loader": "^8.2.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "mustache": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "source-map-loader": "^3.0.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0"
  }
}
